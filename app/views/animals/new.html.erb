<center>
  <%= form_for @animal do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name, class: :"form-control" %>
    <br>
    <%= form.label :image_url %>
    <%= form.file_field :image_url %>
    <br>
    <%= form.submit class: :"btn btn-outline-success" %>
  <% end %>
  <%= link_to 'Back', animals_path %>
</center>